{
  "singleQuote": true,
  "trailingComma": "es5",
  "jsxSingleQuote": true,
  "printWidth": 80,
  "tabWidth": 2,
  "overrides": [
    {
      "files": [
        "**/*.ts",
        "**/*.tsx",
        "**/*.js",
        "**/*.jsx"
      ],
      "options": {
        "parser": "typescript"
      }
    }
  ]
}
